// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider =  "prisma-client-js"
}

datasource db {
  provider =  "sqlite"
  url      =  env("DATABASE_URL")
}

model Curator {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  alias       String   @unique
  url         String   @unique
  totalMovies Int
}

model Movie {
  id          Int      @id @default(autoincrement())
  title       String   
}
